<div class="l-root">
</div>

<div id="start-app"></div>

<% if @user %>
  <script type="application/data" id="user-json">
    <%= @user.html_safe %>
  </script>
<% end %>

<script type="application/data" id="feed-json">
  <%= @feeds ? @feeds.html_safe : [] %>
</script>

<script type="application/data" id="all-feed-json">
  <%= @all_feeds.html_safe %>
