<ul class="song-list">
  {{#each songs}}
    <li class="{{#if listened}} listened {{/if}}
               {{#if playing}}  playing  {{/if}}">
      <a href="#" class="js-switch-song" data-index="{{@index}}">{{title}}</a> --------
      Feed ids -
      {{#each entries}}
        {{feed_id}}
      {{/each}}
    </li>
  {{/each}}
</ul>

<div class="player l-bottom-full">
  <div id="current-song"></div>

  <div class="player__controls">
    <button class="js-navigate-playlist" data-direction="prev">
      <i class="icon-prev"></i>
    </button>
    <button class="js-play">
      <i class="icon-play"></i>
    </button>
    <button class="js-navigate-playlist" data-direction="next">
      <i class="icon-next"></i>
    </button>
  </div>
</div>